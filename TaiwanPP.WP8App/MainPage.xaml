                        <toolkit:MenuItem Header="{Binding favorite, Converter={StaticResource favoriteConverter}}" Click="savestation_Click" FontFamily="Portable User Interface"/>
                        <toolkit:MenuItem Header="查詢該站其他鄰近加油站" Click="querynearbystation_Click" FontFamily="Portable User Interface"/>
                    </toolkit:ContextMenu>
                </toolkit:ContextMenuService.ContextMenu>
